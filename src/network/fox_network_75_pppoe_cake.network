# This configuration file is customized by fox,
# Optimize for PPPoE dialing.

[Match]
Name=pppoe1
Type=ppp

[Network]
IPv6AcceptRA=yes
LLMNR=no

[IPv6AcceptRA]
DHCPv6Client=always
UseDNS=no
UseDomains=no

[DHCPv6]
RapidCommit=yes
UseDNS=no
UseNTP=no
UseDomains=no

[CAKE]
Bandwidth=50M
OverheadBytes=38
MPUBytes=84
UseRawPacketSize=yes
FlowIsolationMode=triple
NAT=yes
PriorityQueueingPreset=diffserv4
RTTSec=60ms
AckFilter=yes

[Link]
RequiredForOnline=yes

